<div class="ons-pl-grid-col"></div>
  {{ $previousReleases := . }}
  {{ $sections := .TableOfContents.Sections }}
  {{ range $id := .TableOfContents.DisplayOrder }}
    {{ $section := index $sections $id }}
    <section id="{{ $id }}">
      <h2>{{ $section.Title }}</h2>
      {{ if eq $id "summary"}}
        <p>{{ $previousReleases.Description.Summary }}</p>
      {{ else if eq $id "releasehistory" }}
        {{ template "partials/previousreleases/contents/release-history" $previousReleases }}
      {{ end }}
    </section>
  {{ end }}
</div>
