<div class="ons-pl-grid-col">
  <section id="summary">
    <h2>Summary</h2>
    <p>...summary...</p>
  </section>

  <section id="publications">
    <h2>Publications</h2>
    {{ template "partial-release-contents-publications" . }}
  </section>

  <section id="data">
    <h2>Data</h2>
    {{ template "partial-release-contents-data" . }}
  </section>

  <section id="contactdetails">
    <h2>Contact details</h2>
    {{ template "partial-release-contents-contact-details" . }}
  </section>

  <section id="changestothisreleasedate">
    <h2>Changes to this release date</h2>
    {{ template "partial-release-contents-date-changes" . }}
  </section>

  <section id="releasehistory">
    <h2>Release history</h2>
    {{ template "partial-release-contents-release-history" . }}
  </section>

  {{ template "partial-release-contents-code-of-practice" . }}
</div>
